#!/usr/bin/env bash
":"; exec emacs --quick --script "$0" -- "$@" # -*- mode: emacs-lisp; lexical-binding: t; -*-
(pop argv)  ; Remove the -- separator

;; eless
(if-let ((dotfiles (getenv "DOTFILES")))
    (url-copy-file "https://raw.githubusercontent.com/kaushalmodi/eless/master/eless" (concat dotfiles "/bin/eless") t))

(load "~/.emacs.d/init.el")

;; Can't run this in a script because it's interactive
;; (update)

(kill-emacs 0)
