function set_fish_colors -d 'set syntax highlighting colors'
  set -gx fish_color_autosuggestion 060
  set -gx fish_color_command 087
  set -gx fish_color_comment 240
  set -gx fish_color_cwd green
  set -gx fish_color_cwd_root red
  set -gx fish_color_end 212
  set -gx fish_color_error 197
  set -gx fish_color_escape cyan
  set -gx fish_color_history_current cyan
  set -gx fish_color_host normal
  set -gx fish_color_match cyan
  set -gx fish_color_normal normal
  set -gx fish_color_operator cyan
  set -gx fish_color_param 099
  set -gx fish_color_quote 166
  set -gx fish_color_redirection 076
  set -gx fish_color_search_match --background=purple
  set -gx fish_color_selection --background=purple
  set -gx fish_color_status red
  set -gx fish_color_user brgreen
  set -gx fish_color_valid_path --underline
end
