function set_fish_colors -d 'set syntax highlighting colors'
  set -gx fish_color_autosuggestion 625B88
  set -gx fish_color_command brcyan
  set -gx fish_color_comment 585858
  set -gx fish_color_cwd green
  set -gx fish_color_cwd_root red
  set -gx fish_color_end brgreen
  set -gx fish_color_error --bold FF0052
  set -gx fish_color_escape bryellow
  set -gx fish_color_history_current cyan
  set -gx fish_color_host yellow
  set -gx fish_color_match blue
  set -gx fish_color_normal normal
  set -gx fish_color_operator brmagenta
  set -gx fish_color_param cyan
  set -gx fish_color_quote E54F00
  set -gx fish_color_redirection bryellow
  set -gx fish_color_search_match --background=purple
  set -gx fish_color_selection --background=blue
  set -gx fish_color_status red
  set -gx fish_color_user brgreen
  set -gx fish_color_valid_path --underline
end
